{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec tsx src/server.ts\"",
    "db:studio": "tsx --tsconfig tsconfig.json node_modules/.bin/drizzle-kit studio",
    "db:push": "tsx --tsconfig tsconfig.json node_modules/.bin/drizzle-kit push",
    "db:migrate": "tsx --tsconfig tsconfig.json node_modules/.bin/drizzle-kit migrate",
    "db:generate": "tsx --tsconfig tsconfig.json node_modules/.bin/drizzle-kit generate",
    "types:copy": "tsx src/scripts/generate-frontend-types.ts",
    "db:generate:sync": "npm run db:generate && npm run types:copy",
    "db:seed": "tsx src/db/seed/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/lib-storage": "^3.917.0",
    "@terraformer/wkt": "^2.2.1",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "@types/terraformer__wkt": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
